{
  "extends": [
    "config:base"
  ],
  "packageRules": [
      {
          "rebaseWhen": "behind-base-branch",
          "matchUpdateTypes": [
              "patch",
              "minor"
          ],
          "automerge": true,
          "lockFileMaintenance": { "enabled": true },
          "labels": ["dependencies"]
      },
      {
        "matchPackageNames": ["uswds"],
        "enabled": false
      }

  ]



}